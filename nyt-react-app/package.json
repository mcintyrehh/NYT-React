{
  "name": "nyt-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.3",
    "express": "^4.16.4",
    "mongoose": "^5.3.7",
    "morgan": "^1.9.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.0",
    "axios": "^0.16.2",
    "if-env": "^1.0.4"
  },
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm run start",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "build": "cd client && react-scripts build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
